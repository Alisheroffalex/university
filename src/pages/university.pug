extends _components/layouts/l-public

append variables
    -
        var page = {
            name: 'university',
            pageTitle: 'Устав университета',
            pageParents: [
                {
                    name: 'Главная',
                    href: '/'
                },
                {
                    name: 'Структура ВУЗа',
                    href: '/'
                }
            ]
